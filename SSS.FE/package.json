{
  "name": "sss.fe",
  "version": "2.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:https": "ng serve --ssl --host=localhost --port=50503",
    "start:ip": "ng serve --host=127.0.0.1 --port=50503",
    "build": "ng build",
    "build:prod": "ng build --configuration=production",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "serve:prod": "ng serve --configuration=production",
    "generate-cert": "node generate-ssl-cert.js",
    "clean": "node clean.js && npm install",
    "clean:only": "node clean.js",
    "clean:windows": "if exist node_modules rmdir /s /q node_modules && if exist package-lock.json del package-lock.json && npm install",
    "clean:unix": "rm -rf node_modules package-lock.json && npm install",
    "fix-deps": "npm install --legacy-peer-deps",
    "postinstall": "npx ngcc --properties es2020 browser module main --first-only --create-ivy-entry-points"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.3.12",
    "@angular/cdk": "^17.3.10",
    "@angular/common": "^17.3.12",
    "@angular/compiler": "^17.3.12",
    "@angular/core": "^17.3.12",
    "@angular/forms": "^17.3.12",
    "@angular/google-maps": "^17.3.10",
    "@angular/material": "^17.3.10",
    "@angular/platform-browser": "^17.3.12",
    "@angular/platform-browser-dynamic": "^17.3.12",
    "@angular/router": "^17.3.12",
    "animate.css": "^4.1.1",
    "aos": "^2.3.4",
    "bootstrap": "^5.3.3",
    "bootstrap-icons": "^1.11.3",
    "chart.js": "^4.5.0",
    "lodash": "^4.17.21",
    "lottie-web": "^5.12.2",
    "moment": "^2.30.1",
    "ng2-charts": "^5.0.4",
    "ngx-lottie": "^10.0.0",
    "ngx-pagination": "^6.0.3",
    "ngx-spinner": "^17.0.0",
    "ngx-toastr": "^18.0.0",
    "rxjs": "~7.8.1",
    "socket.io-client": "^4.8.1",
    "sweetalert2": "^11.14.5",
    "tslib": "^2.6.3",
    "zone.js": "~0.14.10"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.3.10",
    "@angular-eslint/builder": "^17.5.2",
    "@angular-eslint/eslint-plugin": "^17.5.2",
    "@angular-eslint/eslint-plugin-template": "^17.5.2",
    "@angular-eslint/schematics": "^17.5.2",
    "@angular-eslint/template-parser": "^17.5.2",
    "@angular/cli": "^17.3.10",
    "@angular/compiler-cli": "^17.3.12",
    "@types/aos": "^3.0.7",
    "@types/chart.js": "^2.9.41",
    "@types/jasmine": "~5.1.4",
    "@types/lodash": "^4.17.12",
    "@types/node": "^22.9.0",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^8.57.1",
    "jasmine-core": "~5.4.0",
    "karma": "~6.4.4",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "rimraf": "^5.0.5",
    "typescript": "~5.4.5"
  },
  "engines": {
    "node": ">=18.13.0",
    "npm": ">=8.19.0"
  }
}
